{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "globalDependencies": ["**/.env"],
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "db:reset": {
      "dependsOn": ["^db:reset"],
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".cache/tsbuildinfo.json",
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".output/**",
        "dist/**",
        ".vercel/output/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "start": {
      "dependsOn": ["build", "^build"],
      "persistent": true
    },
    "@tonik/supabase#test": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "//#boring-stack-cli": {
      "cache": false
    },
    "generate:types": {
      "cache": false
    },
    "generate:importmap": {
      "cache": false
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "e2e": {
      "cache": false
    },
    "e2e:start": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [
    "DATABASE_URI",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_DATABASE",
    "SUPABASE_JWT_SECRET",
    "POSTGRES_URL",
    "SUPABASE_SERVICE_ROLE_KEY",
    "AUTH_DISCORD_ID",
    "AUTH_DISCORD_SECRET",
    "SUPABASE_URL",
    "SUPABASE_ANON_KEY",
    "NEXT_PUBLIC_VERCEL_URL",
    "NEXT_PUBLIC_VERCEL_BRANCH_URL",
    "NEXT_PUBLIC_VERCEL_PROJECT_PRODUCTION_URL",
    "LOG_LEVEL"
  ],
  "globalPassThroughEnv": [
    "COREPACK_HOME",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "SKIP_ENV_VALIDATION",
    "npm_lifecycle_event"
  ]
}
