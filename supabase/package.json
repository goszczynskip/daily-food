{
  "name": "@tonik/supabase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "db:reset": "supabase db reset",
    "dev": "tsc",
    "generate:types": "supabase gen types typescript --local > src/types.gen.ts",
    "format": "prettier --check . --ignore-path ../.gitignore --ignore-path ./.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env supabase start",
    "stop": "supabase stop",
    "test": "supabase test db",
    "supabase": "pnpm with-env supabase",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@supabase/auth-js": "^2.71.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.57.4",
    "supabase": "^2.67.2",
    "tsx": "^4.21.0"
  },
  "peerDependencies": {
    "next": "catalog:next",
    "react": "catalog:react",
    "react-dom": "catalog:react"
  },
  "devDependencies": {
    "@tonik/eslint-config": "workspace:*",
    "@tonik/prettier-config": "workspace:*",
    "@tonik/tsconfig": "workspace:*",
    "@types/react": "catalog:react",
    "dotenv-cli": "^11.0.0",
    "eslint": "catalog:",
    "next": "catalog:react",
    "prettier": "catalog:",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "typescript": "catalog:"
  },
  "prettier": "@tonik/prettier-config"
}
