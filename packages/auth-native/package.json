{
  "name": "@tonik/auth-native",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./recipes/*": {
      "types": "./dist/src/recipes/*.d.ts",
      "default": "./src/recipes/*.tsx"
    },
    "./login-handlers/*": {
      "types": "./dist/src/login-handlers/*.d.ts",
      "default": "./src/login-handlers/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "catalog:native",
    "@react-native-google-signin/google-signin": "catalog:native",
    "@standard-schema/spec": "^1.1.0",
    "@tonik/auth": "workspace:*",
    "@tonik/supabase": "workspace:*",
    "@tonik/ui-native": "workspace:*",
    "expo-linking": "catalog:native",
    "nativewind": "catalog:native",
    "react": "catalog:native",
    "react-dom": "catalog:native",
    "react-native": "catalog:native",
    "react-native-css": "catalog:native",
    "zod": "catalog:",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tonik/eslint-config": "workspace:*",
    "@tonik/prettier-config": "workspace:*",
    "@tonik/tsconfig": "workspace:*",
    "@types/react": "catalog:native",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@tonik/prettier-config"
}
